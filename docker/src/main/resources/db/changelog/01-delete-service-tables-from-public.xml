<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="delete-liquibase-tables-from-public" author="svetlana koreneva">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="databasechangelog" schemaName="public"/>
            <tableExists tableName="databasechangeloglock" schemaName="public"/>
        </preConditions>
        <sql>DROP TABLE public.databasechangelog;</sql>
        <sql>DROP TABLE public.databasechangeloglock;</sql>
    </changeSet>
</databaseChangeLog>